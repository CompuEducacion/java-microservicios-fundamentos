<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:xalan="http://xml.apache.org/xslt" xmlns:j2seproject3="http://www.netbeans.org/ns/j2se-project/3" xmlns:jaxws="http://www.netbeans.org/ns/jax-ws/1">
    <!--
                ===================
                JAX-WS WSIMPORT SECTION
                ===================
            -->
    <target name="wsimport-init" depends="init">
        <mkdir dir="${build.generated.sources.dir}/jax-ws"/>
        <taskdef name="wsimport" classname="com.sun.tools.ws.ant.WsImport">
            <classpath path="${libs.jaxws21.classpath}"/>
        </taskdef>
    </target>
    <target name="wsimport-client-StockPriceService" depends="wsimport-init">
        <mkdir dir="${build.generated.dir}/jax-wsCache/StockPriceService"/>
        <property name="wsdl-StockPriceService" location="xml-resources/web-service-references/StockPriceService/wsdl/localhost_8080/BrokerTool/StockPriceService.wsdl"/>
        <wsimport sourcedestdir="${build.generated.dir}/jax-wsCache/StockPriceService" destdir="${build.generated.dir}/jax-wsCache/StockPriceService" package="webservicetester" wsdl="${wsdl-StockPriceService}" catalog="catalog.xml" extension="true" verbose="true" wsdlLocation="http://localhost:8080/BrokerTool/StockPriceService?WSDL" xnocompile="true" xendorsed="true">
            <depends file="${wsdl-StockPriceService}"/>
            <produces dir="${build.generated.dir}/jax-wsCache/StockPriceService"/>
        </wsimport>
        <copy todir="${build.generated.sources.dir}/jax-ws">
            <fileset dir="${build.generated.dir}/jax-wsCache/StockPriceService">
                <include name="**/*.java"/>
            </fileset>
        </copy>
    </target>
    <target name="wsimport-client-clean-StockPriceService" depends="-init-project">
        <delete dir="${build.generated.dir}/jax-wsCache/StockPriceService"/>
        <delete dir="${build.generated.sources.dir}/jax-ws/webservicetester"/>
    </target>
    <target name="wsimport-client-generate" depends="wsimport-client-StockPriceService"/>
</project>
